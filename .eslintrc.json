{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "strict": ["error", "never"],
        "valid-typeof":"error",
        "no-unsafe-negation": "error",
        "no-unused-vars": "error",
        "no-unexpected-multiline": "error",
        "no-undef": "error"
    }
}
